public double d1;
public double d3;
public bool is_near;
public double dX;
public double dY;
private double assign;

@construct {
  transition #setup;
}

#setup {
  d1 = 3.14;
  double d2 = 2.71;
  d3 = d1 + d2;
  double d4 = 100.0e4;
  double d5 = d4 * d3;
  double x = 0.1;
  x += 0.2;
  is_near = x == 0.3;
  dX = x;
  dY = 0.3;
  string foo = 0.1 + "x" + 0.2;

  assign = 0;
  assign += 100;
  assign /= 2;
  assign *= 3;
  assign -= 17;
}

@connected (who) {
  return who == @no_one;
}